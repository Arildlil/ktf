ccflags-y += -Wno-declaration-after-statement

KVER = @KVER@
KTF_DIR = @KTF_DIR@
KTF_BDIR = @KTF_BDIR@

ccflags-y += -I$(KTF_DIR)

obj-m := selftest.o

-include ktf_gen.mk

selftest-y := self.o hybrid.o

KDIR   := @KDIR@
PWD    := $(shell pwd)

EXTRASYMS := KBUILD_EXTRA_SYMBOLS="$(KTF_BDIR)/Module.symvers"

module:
	$(MAKE) -C $(KDIR) SUBDIRS=$(PWD) $(EXTRASYMS) modules
clean:
	$(MAKE) -C $(KDIR) M=$(PWD) clean

check: all
install: all
