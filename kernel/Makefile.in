ccflags-y += -Wno-declaration-after-statement

KVER = @KVER@

-include ktf_gen.mk

obj-m := ktf.o

ktf-y := kbase.o nl.o ktf_map.o kcheck.o ktf_debugfs.o

KDIR   := /lib/modules/$(KVER)
PWD    := $(shell pwd)

module:
	$(MAKE) -C $(KDIR)/build SUBDIRS=$(PWD) modules
clean:
	$(MAKE) -C $(KDIR)/build M=$(PWD) clean

check: all
install: all
